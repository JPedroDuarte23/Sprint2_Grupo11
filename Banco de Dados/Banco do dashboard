use sprint2;
create table sensor(
idSensor int primary key auto_increment,
tipo varchar(20),
temperatura int,
umidade int,
dtAtual DATETIME default current_timestamp 
);

insert into sensor (idSensor,tipo,temperatura,umidade) values
(null, 'DHT11', '5', '86'),
(null, 'DHT11', '3', '90'),
(null, 'DHT11', '2', '89'),
(null, 'DHT11', '4', '91'),
(null, 'DHT11', '5', '88');

select * from sensor;
